{
    "name": "@thermopylae/lib.cache",
    "description": "Modular synchronous cache.",
    "keywords": [
        "thermopylae",
        "library",
        "cache"
    ],
    "version": "0.0.1587317824",
    "author": "Rusu Marin <dimarusu2000@gmail.com>",
    "main": "dist/index.js",
    "exports": "./dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "lint": "eslint \"lib/**/*.ts\" \"test/**/*.ts\" --fix",
        "build": "gulp build",
        "doc": "gulp doc",
        "test": "mocha --expose-gc",
        "report": "gulp report",
        "coverage": "gulp coverage",
        "coverage:show": "gulp coverageShow",
        "debug": "gulp debug",
        "clean": "gulp clean",
        "purge": "gulp purge",
        "update": "gulp stickToMajorSemver && npm update --dev --force -verbose",
        "bumpVersion": "timestamp=$(date \"+%s\") && npm version 0.0.$timestamp",
        "precommit": "npm run lint",
        "prepublishOnly": "npm run lint && npm run clean && npm run build && npm run bumpVersion && gulp stickToMajorSemver && gulp readme",
        "unpublish": "npm unpublish --force $npm_package_name"
    },
    "dependencies": {
        "@thermopylae/lib.exception": "0",
        "@thermopylae/lib.heap": "0",
        "@thermopylae/lib.pool": "0",
        "@thermopylae/lib.utils": "0"
    },
    "devDependencies": {
        "@thermopylae/core.declarations": "0",
        "@thermopylae/lib.unit-test": "0",
        "@thermopylae/module-builder": "0",
        "@types/array-move": "2",
        "@types/dotenv": "8",
        "array-move": "3",
        "dotenv": "8",
        "js-gc": "1"
    },
    "peerDependencies": {
        "mocha": "8",
        "chai": "4"
    },
    "lint-staged": {
        "*.ts": [
            "eslint"
        ]
    },
    "engines": {
        "node": ">=14"
    },
    "license": "ISC",
    "type": "commonjs"
}
