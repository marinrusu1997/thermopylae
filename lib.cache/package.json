{
    "name": "@thermopylae/lib.cache",
    "description": "Memory cache",
    "keywords": [
        "events",
        "cache",
        "memory"
    ],
    "version": "0.0.1587317824",
    "author": "Rusu Marin <dimarusu2000@gmail.com>",
    "license": "ISC",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "lint": "eslint \"lib/**/*.ts\" \"test/**/*.ts\" --fix",
        "build": "gulp build",
        "test": "gulp test",
        "coverage": "gulp coverage",
        "coverage:show": "gulp coverageShow",
        "debug": "gulp debug",
        "clean": "gulp clean",
        "purge": "gulp purge",
        "update": "gulp stickToMajorSemver && npm update --dev --force -verbose",
        "bumpVersion": "timestamp=$(date \"+%s\") && npm version 0.0.$timestamp",
        "prepublishOnly": "npm run lint && npm run clean && npm run build && npm run bumpVersion && gulp stickToMajorSemver"
    },
    "lint-staged": {
        "*.ts": [
            "eslint"
        ]
    },
    "engines": {
        "node": ">= 12"
    },
    "dependencies": {
        "@thermopylae/lib.collections": "0",
        "@thermopylae/lib.exception": "0",
        "@thermopylae/lib.utils": "0",
        "lodash.debounce": "4"
    },
    "devDependencies": {
        "@thermopylae/core.declarations": "0",
        "@thermopylae/module-builder": "0",
        "@types/chai-arrays": "1",
        "@types/dotenv": "8",
        "@types/lodash.debounce": "4",
        "@types/redis": "2",
        "chai-arrays": "2",
        "dotenv": "8"
    }
}
