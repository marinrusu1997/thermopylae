{
    "name": "@thermopylae/lib.utils",
    "description": "Utility functions",
    "keywords": [
        "utils",
        "shared"
    ],
    "version": "0.0.1592849945",
    "author": "Rusu Marin <dimarusu2000@gmail.com>",
    "license": "ISC",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "lint": "eslint \"lib/**/*.ts\" \"test/**/*.ts\" --fix",
        "build": "gulp build",
        "test": "gulp test",
        "coverage": "gulp coverage",
        "coverage:show": "gulp coverageShow",
        "debug": "gulp debug",
        "clean": "gulp clean",
        "purge": "gulp purge",
        "update": "gulp stickToMajorSemver && npm update --dev --force -verbose",
        "bumpVersion": "timestamp=$(date \"+%s\") && npm version 0.0.$timestamp",
        "prepublishOnly": "npm run lint && npm run clean && npm run build && npm run bumpVersion && gulp stickToMajorSemver"
    },
    "lint-staged": {
        "*.ts": [
            "eslint"
        ]
    },
    "engines": {
        "node": ">=12"
    },
    "dependencies": {
        "@thermopylae/lib.exception": "0",
        "@types/lodash.clone": "4",
        "@types/lodash.clonedeep": "4",
        "lodash.clone": "4",
        "lodash.clonedeep": "4",
        "otplib": "11",
        "tiny-async-pool": "1",
        "uuid": "7"
    },
    "devDependencies": {
        "@thermopylae/core.declarations": "0",
        "@thermopylae/module-builder": "0",
        "@types/chai-arrays": "1",
        "@types/chai-as-promised": "7",
        "@types/otplib": "10",
        "@types/tiny-async-pool": "1",
        "@types/uuid": "7",
        "chai-arrays": "2",
        "chai-as-promised": "7"
    },
    "peerDependencies": {
        "mocha": "8",
        "chai": "4"
    }
