{
    "name": "@thermopylae/lib.unit-test",
    "description": "Unit tests environment",
    "keywords": [
        "thermopylae",
        "library"
    ],
    "version": "0.0.1604217724",
    "author": "Rusu Marin dimarusu2000@gmail.com",
    "main": "dist/index.js",
    "exports": {
        ".": "./dist/index.js",
        "./dist/fixtures/person": "./dist/fixtures/person/index.js",
        "./dist/logger": "./dist/logger.js"
    },
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "lint": "eslint \"lib/**/*.ts\" \"test/**/*.ts\" --fix",
        "build": "gulp build",
        "test": "gulp test",
        "coverage": "gulp coverage",
        "coverage:show": "gulp coverageShow",
        "debug": "gulp debug",
        "clean": "gulp clean",
        "purge": "gulp purge",
        "update": "gulp stickToMajorSemver && npm update --dev --force -verbose",
        "bumpVersion": "timestamp=$(date \"+%s\") && npm version 0.0.$timestamp",
        "precommit": "npm run lint",
        "prepublishOnly": "npm run lint && npm run clean && npm run build && npm run bumpVersion && gulp stickToMajorSemver && gulp readme",
        "unpublish": "npm unpublish --force $npm_package_name"
    },
    "dependencies": {
        "@thermopylae/lib.logger": "0",
        "@thermopylae/lib.utils": "0",
        "@types/chai-arrays": "1",
        "@types/chai-as-promised": "7",
        "chai-arrays": "2",
        "chai-as-promised": "7",
        "chai-asserttype": "1",
        "chai-date-string": "0",
        "deep-freeze": "0",
        "json-schema-typed": "7",
        "mocker-data-generator": "2"
    },
    "devDependencies": {
        "@thermopylae/core.declarations": "0",
        "@thermopylae/module-builder": "0",
        "@types/deep-freeze": "0"
    },
    "peerDependencies": {
        "mocha": "8",
        "chai": "4"
    },
    "lint-staged": {
        "*.ts": [
            "eslint"
        ]
    },
    "engines": {
        "node": ">= 14"
    },
    "license": "ISC",
    "type": "commonjs"
}
