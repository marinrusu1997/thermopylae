{
  "name": "USER_SERVICE",
  "path": "/user",
  "methods": {
    "create": {
      "method": "post",
      "path": "/new"
    },
    "read": {
      "method": "get",
      "unauthorized": "[/]?$"
    },
    "update": {
      "method": "put",
      "path": "/update/:birth/and/:age",
      "unauthorized": "(?:\\/update\\b)(?:\\/[\\w]+)(?:\\/and\\b)(?:\\/[\\w]+)$"
    },
    "about": {
      "method": "options",
      "path": "/about/",
      "unauthorized": "/about$"
    },
    "delete": {
      "method": "delete",
      "path": "/remove/all/users"
    }
  }
}
