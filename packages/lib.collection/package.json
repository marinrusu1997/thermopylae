{
    "name": "@thermopylae/lib.collection",
    "description": "MongoDB like collection.",
    "keywords": [
        "thermopylae",
        "library",
        "mongoose",
        "collection"
    ],
    "version": "0.0.1",
    "author": "Rusu Marin dimarusu2000@gmail.com",
    "main": "dist/index.js",
    "exports": "./dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "lint": "eslint \"lib/**/*.ts\" \"test/**/*.ts\" --fix",
        "build": "gulp build",
        "doc": "gulp doc",
        "test": "gulp test",
        "report": "gulp report",
        "coverage": "gulp coverage",
        "coverage:show": "gulp coverageShow",
        "debug": "gulp debug",
        "clean": "gulp clean",
        "purge": "gulp purge",
        "update": "gulp stickToMajorSemver && npm update --dev --force -verbose",
        "bumpVersion": "timestamp=$(date \"+%s\") && npm version 0.0.$timestamp",
        "precommit": "npm run lint",
        "prepublishOnly": "npm run lint && npm run clean && npm run build && npm run bumpVersion && gulp stickToMajorSemver && gulp readme",
        "unpublish": "npm unpublish --force $npm_package_name"
    },
    "dependencies": {
        "@b4dnewz/mongodb-operators": "0",
        "@thermopylae/core.declarations": "0",
        "@thermopylae/lib.exception": "0",
        "@thermopylae/lib.indexed-store": "0",
        "ajv": "6",
        "ajv-i18n": "3",
        "common-query": "3",
        "dot-prop": "6",
        "isobject": "4",
        "json-schema-typed": "7",
        "lodash.orderby": "4",
        "rx-subject": "3"
    },
    "devDependencies": {
        "@thermopylae/lib.unit-test": "0",
        "@thermopylae/lib.utils": "0",
        "@thermopylae/module-builder": "0",
        "@types/lodash.orderby": "4",
        "@types/lodash.uniqby": "4",
        "array-differ": "3",
        "array-find-duplicates": "2",
        "lodash.uniqby": "4",
        "range-generator": "1",
        "ts-enum-util": "4"
    },
    "peerDependencies": {
        "chai": "4",
        "mocha": "8"
    },
    "lint-staged": {
        "*.ts": [
            "eslint"
        ]
    },
    "engines": {
        "node": ">= 14"
    },
    "license": "ISC",
    "type": "commonjs"
}