{
    "name": "@thermopylae/lib.authentication",
    "description": "Authentication library.",
    "keywords": [
        "library"
    ],
    "version": "0.0.1",
    "author": "Rusu Marin <dimarusu2000@gmail.com>",
    "main": "dist/index.js",
    "exports": "./dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "lint": "eslint \"lib/**/*.ts\" \"test/**/*.ts\" --fix",
        "build": "gulp build",
        "doc": "gulp doc",
        "readme": "gulp readme",
        "test": "gulp test",
        "report": "gulp report",
        "coverage": "gulp coverage",
        "coverage:show": "gulp coverageShow",
        "debug": "gulp debug",
        "clean": "gulp clean",
        "purge": "gulp purge",
        "update": "gulp stickToMajorSemver && npm update --dev --force -verbose",
        "bumpVersion": "timestamp=$(date \"+%s\") && npm version 0.0.$timestamp",
        "precommit": "npm run lint",
        "prepublishOnly": "npm run lint && npm run clean && npm run build && npm run bumpVersion && gulp stickToMajorSemver && gulp readme",
        "unpublish": "npm unpublish --force $npm_package_name"
    },
    "dependencies": {
        "@otplib/core": "12",
        "@otplib/plugin-crypto": "12",
        "@otplib/plugin-thirty-two": "12",
        "@thermopylae/core.declarations": "0",
        "@thermopylae/lib.exception": "0",
        "argon2": "0",
        "node-fetch": "2",
        "qrcode": "1",
        "string-similarity": "4",
        "uid-safe": "2",
        "zxcvbn": "4"
    },
    "devDependencies": {
        "@thermopylae/lib.cache": "0",
        "@thermopylae/dev.unit-test": "0",
        "@thermopylae/lib.utils": "0",
        "@thermopylae/dev.environment": "0",
        "@types/bcrypt": "5",
        "@types/mongoose": "5",
        "@types/node-fetch": "2",
        "@types/object-hash": "2",
        "@types/qrcode": "1",
        "@types/string-similarity": "4",
        "@types/uid-safe": "2",
        "@types/zxcvbn": "4",
        "bcrypt": "5",
        "keypair": "1",
        "mongodb-memory-server": "7",
        "mongoose": "5",
        "object-hash": "2"
    },
    "peerDependencies": {
        "chai": "4",
        "mocha": "8"
    },
    "lint-staged": {
        "*.ts": [
            "eslint"
        ]
    },
    "engines": {
        "node": ">= 16"
    },
